{
  "name": "real-estate-landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:watch": "nodemon --exec \"next dev --turbopack\"",
    "optimize-images": "node scripts/optimize-images.js",
    "build": "npm run optimize-images && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "start:watch": "nodemon --exec \"next start\"",
    "lint": "next lint",
    "update-images": "node scripts/download-premium-images.js",
    "predeploy": "npm run update-images && npm run build",
    "deploy": "gh-pages -d out"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-share": "^5.2.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^6.3.0",
    "next-sitemap": "^4.2.3",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.23",
    "postcss-import": "^16.1.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "webpack": "^5.99.9"
  }
}
